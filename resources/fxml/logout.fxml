<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="130.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LogoutController">
   <children>
      <HBox prefHeight="130.0" prefWidth="108.0" style="-fx-background-color: #adff2f;" />
      <HBox alignment="TOP_RIGHT" layoutX="107.0" prefHeight="129.0" prefWidth="115.0" style="-fx-background-color: #ffffff;">
         <children>
            <VBox prefHeight="129.0" prefWidth="100.0" />
            <ImageView fitHeight="38.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pictures/logout.png" />
               </image></ImageView>
         </children>
      </HBox>
      <Label fx:id="label_Message" alignment="CENTER" layoutX="37.0" layoutY="8.0" prefHeight="58.0" prefWidth="129.0" text="Are you sure?" />
      <Button fx:id="yes_BTN" layoutX="37.0" layoutY="56.0" mnemonicParsing="false" onAction="#onYes" prefHeight="25.0" prefWidth="51.0" text="Yes" />
      <Button fx:id="no_BTN" layoutX="124.0" layoutY="56.0" mnemonicParsing="false" onAction="#onNo" prefHeight="25.0" prefWidth="51.0" text="No" />
   </children>
</AnchorPane>
